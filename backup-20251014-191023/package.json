{
  "name": "rizzify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run worker\"",
    "dev:web": "next dev -p 3000",
    "dev:web-only": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "tsx src/worker/task-worker.ts",
    "db:validate": "prisma validate",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:smoke": "tsx scripts/db-smoke-test.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:cleanup": "tsx scripts/cleanup-expired-photos.ts",
    "db:rollover": "tsx scripts/quotas-rollover.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.907.0",
    "@prisma/client": "^6.16.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/uuid": "^11.0.0",
    "aws-sdk": "^2.1692.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^10.16.0",
    "next": "14.0.0",
    "pg-boss": "^10.3.3",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8",
    "prisma": "^6.16.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
