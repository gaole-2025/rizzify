{
  "name": "rizzify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:with-worker": "concurrently -n \"WEB,WORKER\" -c \"cyan,green\" \"npm run dev:web\" \"npm run worker\" --kill-others-on-fail",
    "dev:web": "next dev -p 3000",
    "dev:web-only": "next dev -p 3000",
    "dev:mock": "concurrently -n \"WEB,MOCK\" -c \"cyan,yellow\" \"npm run dev:web\" \"npm run worker:mock\" --kill-others-on-fail",
    "dev:clean": "npm run clean && npm run dev",
    "clean": "if exist .next rmdir /s /q .next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "tsx src/worker/start-real-worker.ts",
    "worker:mock": "tsx src/worker/task-worker.ts",
    "db:validate": "prisma validate",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:smoke": "tsx scripts/db-smoke-test.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:cleanup": "tsx scripts/cleanup-expired-photos.ts",
    "db:rollover": "tsx scripts/quotas-rollover.ts",
    "test:api": "tsx scripts/test-api-integration.ts",
    "migrate-images": "node scripts/migrate-images-to-r2.js",
    "migrate-images:test": "node -e \"console.log(require('./lib/image-urls').getImageSourceInfo())\"",
    "build-catalogs": "node scripts/build-catalogs.js",
    "avatars:setup": "node setup-avatars.js",
    "avatars:convert": "node convert-avatars-simple.js",
    "avatars:upload": "node scripts/process-avatars.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.907.0",
    "@prisma/client": "^6.16.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/uuid": "^11.0.0",
    "aws-sdk": "^2.1692.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^10.16.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.546.0",
    "next": "^15.0.0",
    "pg-boss": "^10.3.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8",
    "prisma": "^6.16.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
